{
  "functions": {
    "src/api/**/*.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/show",
      "destination": "/show.html"
    },
    {
      "source": "/services",
      "destination": "/services.html"
    },
    {
      "source": "/contactus",
      "destination": "/contactus/index.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/blog",
      "destination": "/blog.html"
    },
    {
      "source": "/case-studies",
      "destination": "/case-studies.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": false
    }
  ]
}
